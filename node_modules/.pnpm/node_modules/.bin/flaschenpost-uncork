#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/flaschenpost@1.1.3/node_modules/flaschenpost/dist/bin/node_modules:/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/flaschenpost@1.1.3/node_modules/flaschenpost/dist/node_modules:/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/flaschenpost@1.1.3/node_modules/flaschenpost/node_modules:/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/flaschenpost@1.1.3/node_modules:/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/flaschenpost@1.1.3/node_modules/flaschenpost/dist/bin/node_modules:/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/flaschenpost@1.1.3/node_modules/flaschenpost/dist/node_modules:/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/flaschenpost@1.1.3/node_modules/flaschenpost/node_modules:/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/flaschenpost@1.1.3/node_modules:/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../flaschenpost/dist/bin/flaschenpost-uncork.js" "$@"
else
  exec node  "$basedir/../flaschenpost/dist/bin/flaschenpost-uncork.js" "$@"
fi
