#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/astro@5.14.7_@types+node@24_1f1744bd2f55a409b379b0ee2df53e1a/node_modules/astro/node_modules:/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/astro@5.14.7_@types+node@24_1f1744bd2f55a409b379b0ee2df53e1a/node_modules:/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/astro@5.14.7_@types+node@24_1f1744bd2f55a409b379b0ee2df53e1a/node_modules/astro/node_modules:/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/astro@5.14.7_@types+node@24_1f1744bd2f55a409b379b0ee2df53e1a/node_modules:/mnt/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.14.7_@types+node@24_1f1744bd2f55a409b379b0ee2df53e1a/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.14.7_@types+node@24_1f1744bd2f55a409b379b0ee2df53e1a/node_modules/astro/astro.js" "$@"
fi
