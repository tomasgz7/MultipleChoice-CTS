#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/astro@5.14.8_@types+node@24_1d3b3dbbcdd21d894cd984f8fff9ae5e/node_modules/astro/node_modules:/proc/cygdrive/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/astro@5.14.8_@types+node@24_1d3b3dbbcdd21d894cd984f8fff9ae5e/node_modules:/proc/cygdrive/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/astro@5.14.8_@types+node@24_1d3b3dbbcdd21d894cd984f8fff9ae5e/node_modules/astro/node_modules:/proc/cygdrive/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/astro@5.14.8_@types+node@24_1d3b3dbbcdd21d894cd984f8fff9ae5e/node_modules:/proc/cygdrive/c/Users/Klein/CascadeProjects/trabajo-tecnologia-sociedad/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
